{
  "kind": "collectionType",
  "collectionName": "items",
  "info": {
    "name": "Item",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "excerpt": {
      "type": "string",
      "maxLength": 250
    },
    "content": {
      "type": "text"
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    },
    "created": {
      "type": "date"
    },
    "typ": {
      "type": "enumeration",
      "enum": [
        "turnieje",
        "zory",
        "lubin"
      ]
    }
  }
}
